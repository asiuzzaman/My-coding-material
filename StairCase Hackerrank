#include <bits/stdc++.h>
using namespace std;
#define read() freopen("/home/asiuzzaman/Documents/input.txt", "r", stdin);
#define write() freopen("/home/asiuzzaman/Documents/output.txt", "w", stdout);
int main()
{
	#ifdef asiuzzaman
	read(); write();
	#endif //asiuzzaman
	int n;
	cin>>n;
	for (int i = 0; i < n; ++i)
	{
		for (int j = n-1; j >=0; --j)
		{
			if(j>i) cout<<" ";
			else cout<<"#";
		}
		cout<<endl;
	}
}
